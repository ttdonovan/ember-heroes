<div class="mdl-grid">
  <div class="mdl-cell mdl-cell--12-col">
    <h2>{{model.name}}</h2>
  </div>
  <div class="mdl-cell mdl-cell--12-col">
    <small>{{model.description}}</small>
  </div>
  <div class="mdl-cell mdl-cell--12-col">
    <img src={{model.thumbnail}} />
  </div>
  <div class="mdl-cell mdl-cell--12-col">
    <small>{{model.resourceURI}}</small>
  </div>
</div>
